{
  "expo": {
    "name": "todoApp",
    "slug": "todoapp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.techtypo.todoapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.techtypo.todoapp",
      "versionCode": 1
    },
    "description": "A simple Todo App to store your daily todoes....",
    "githubUrl": "https://github.com/Abhi204655/todoAppReactNative"
  }
}